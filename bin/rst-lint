#!/Users/yuki/MEGA/src/pandoc/pandoc-jp/pandoc-doc-ja/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'restructuredtext-lint==1.2.1','console_scripts','rst-lint'
__requires__ = 'restructuredtext-lint==1.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('restructuredtext-lint==1.2.1', 'console_scripts', 'rst-lint')()
    )
